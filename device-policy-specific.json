{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": [
          "iot:Connect"
        ],
        "Resource": [
          "arn:aws:iot:us-east-1:506189848464:client/${iot:ClientId}"
        ]
      },
      {
        "Effect": "Allow",
        "Action": [
          "iot:Publish"
        ],
        "Resource": [
          "arn:aws:iot:us-east-1:506189848464:topic/aurangabad/temperature",
          "arn:aws:iot:us-east-1:506189848464:topic/aurangabad/humidity"
  
        ]
      }
    ]
  }